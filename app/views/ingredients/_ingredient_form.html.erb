<%= form_for(@ingredient) do |f| %>
    <%= render 'shared/error_messages', object: f.object %>
    <div class="field">
      <%= f.text_field :name, placeholder: 'Ingredient name...' %>
    </div>
    <div class="field">
      <%= f.select :expiration_date_id, [['Test 1', 1], ['Test 2', 2], ['Test 3', 3]]%>
    </div>
    <div class="field">
      <%= f.datetime_select :purchase_date %>
    </div>
    <%= f.submit 'Add', class: 'btn btn-primary' %>
<% end %>